{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#29315A",
          "foreground": "#00D0DB",
          "leading_diamond": "\ue0b6",      
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b",
            "windows": "\uf871",
            "wsl": "\ue712"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": "{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#29315A",
          "foreground": "#E64747",
          "style": "diamond",
          "leading_diamond": "\ue0b6",      
          "template": "{{.UserName}}",
          "type": "session"
        },
        {
          "background": "#29315A",
          "foreground": "#F106E3",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },        
        {
          "background": "#29315A",
          "foreground": "#43CCEA",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "leading_diamond": "\ue0b6",
          "properties": {
            "branch_max_length": 0,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }        
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [        
          {
            "background": "#29315A",
            "foreground": "#E4F34A",
            "leading_diamond": "\ue0b6",
            "properties": {
              "fetch_version": false
            },
            "style": "diamond",
            "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4",
            "type": "python"
          },
          {
            "background": "#29315A", 
            "foreground": "#FE4A49",
            "leading_diamond": "\ue0b6", 
            "properties": {
              "fetch_version": true
            },
            "style": "diamond",         
            "template": "\ue71e {{ .Major }}.{{ .Minor }}",
            "trailing_diamond": "\ue0b4",
            "type": "npm"
          },
          {
            "background": "#29315A",
            "foreground": "#42E66C",
            "leading_diamond": "\ue0b6",
            "properties": {
              "fetch_version": true
            },
            "style": "diamond",
            "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}{{ .Major }}.{{ .Minor }}",
            "trailing_diamond": "\ue0b4",
            "type": "node"
          },
          {
            "background": "#29315A",
            "foreground": "#E64747",
            "leading_diamond": "\ue0b6",
            "properties": {
              "fetch_version": false
            },
            "style": "diamond",
            "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4",
            "type": "ruby"
          },
          {
            "background": "#29315A",
            "foreground": "#E64747",
            "leading_diamond": " \ue0b6",
            "properties": {
              "fetch_version": false
            },
            "style": "diamond",
            "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4",
            "type": "java"
          },         
          {
            "background": "#29315A",
            "foreground": "#AEA4BF",
            "leading_diamond": "\ue0b6",
            "properties": {
              "style": "austin",
              "threshold": 0,
              "always_enabled": true
            },
            "background_templates": [
              "{{ if gt .Code 0 }}#D51111{{ end }}"
            ],
            "style": "diamond",
            "template": "{{ .FormattedMs }}",
            "trailing_diamond": "\ue0b4",
            "type": "executiontime"   
          }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {          
          "background":"#29315A",
          "foreground":"#7FD5EA",
          "leading_diamond": "\ue0b6",
          "template": "\u276f",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}